        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
        <Button Content="Send" HorizontalAlignment="Left" Margin="257,200,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" />
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="134" Margin="72,169,0,0" TextWrapping="Wrap" Text="cmd.exe" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
